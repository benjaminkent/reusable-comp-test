{
    "compilerOptions": {
        "target": "es5",
        "strict": true,
        "module": "es2015",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "noImplicitThis": true,
        "noImplicitReturns": true,
        "experimentalDecorators": true
    },
    "paths": {
        "@/*": [
            "components/*"
        ]
    },
    "include": [
        "components/**/*.ts"
    ],
    "files": [
        "./shims-vue.d.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}